script/source = "extends Area2D
@onready var door: AudioStreamPlayer = $door

func _on_body_entered(body):
	if body.name == \"Player\" and body.has_key == true:
		queue_free()
		var key = body.get_node_or_null(\"Key\")
		if key:
			door.play()
			key.queue_free()
			get_tree().reload_current_scene()

	
"

